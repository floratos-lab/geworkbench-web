	<script src="js/jquery-1.4.4.min.js" />
   	<script src="js/highcharts.js" />
   	<script src="js/exporting.js" />  
   	<script src="js/msviper.js" />

   	<!-- custom javascript stuff --> <!-- js is a sub-directory under visualizations/public; name js is not required -->
   	<script src="js/ChemDoodleWeb.js" />
   	<script src="js/molecule_viewer.js" />
	
	<!-- Invient Charts Addon -->
   	<inherits name="com.invient.vaadin.charts.widgetset.InvientChartsWidgetset" />
    
    <!-- Cytoscape.js and the network viewer based on it -->
	<script src="js/cytoscape.min.js" />
	<script src="js/jquery.min.js" />
	<script src="js/jquery.contextMenu.min.js" />
	<script src="js/network_viewer.js" />
	
	<!--  interaction color mosaic -->
	<script src="js/interaction_color_mosaic.js" />

	<!--  citrus diagram -->
	<script src="js/d3.v3.min.js" />
	<script src="js/citrus_diagram.js" />

    <inherits name="com.vaadin.addon.tableexport.widgetset.Tableexport_for_vaadinWidgetset" />

    <inherits name="com.github.wolfie.sessionguard.SessionguardWidgetset" />

    <inherits name="org.vaadin.artur.icepush.IcepushaddonWidgetset" />

    <inherits name="org.vaadin.alump.fancylayouts.gwt.FancyLayoutsWidgetset" />

</module>
